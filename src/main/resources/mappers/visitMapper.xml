<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="visitMapper">
	<insert id="insertIp">
		INSERT INTO visit(
		visit_ip
		, visit_time)
		VALUES(
		#{visit_ip}
		, #{visit_time} )
	</insert>
</mapper>